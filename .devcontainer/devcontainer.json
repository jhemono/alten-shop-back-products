{
	"name": "Product Trial",

	"dockerComposeFile": [
		"../docker-compose.yaml",
		"docker-compose.yml"
	],

	"service": "back",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	
	"features": {
		"ghcr.io/devcontainers/features/git:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.vscode-pylance",
				"ms-python.python",
				"ms-python.debugpy",
				"EditorConfig.EditorConfig",
				"ms-python.black-formatter",
				"ms-python.isort"
			]
		}
	}
}
